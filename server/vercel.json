 
{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/socket.io",
        "dest": "/"
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "OPTIONS"
        ]
      }
    ]
  }
  